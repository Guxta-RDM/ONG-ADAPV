<script src="/js/listar/ctrlSaidaEvento.js"></script>

<h1>Voluntarios Registrados</h1>
<a class="btn btn-secondary" style="max-width: 200px;" href="/ctrlSaidaEvento/cadastrar">Novo Voluntário</a>
<hr>
    <div>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Descrição</th>
                    <th>Estado</th>
                    <th>Id de Saída do Produto</th>
                    <th>Quantidade de Saída do Produto</th>
                    <th>Id de Entrada do Produto</th>
                    <th>Quantidade de Entrada do Produto</th>
                    <th>Id do Evento</th>
                    <th>Data de Registro</th>
                    <th>Data de Atualização</th>
                </tr>
            </thead>
            </thead>
            <tbody>
                <% for (let i = 0; i < listaCtrlEven.length; i++)  { %>
                    <tr>
                        <td><%= listaCtrlEven[i].ctrlEven_id %></td>
                        <td><%= listaCtrlEven[i].ctrlEven_desc %></td>
                        <td><%= listaCtrlEven[i].ctrlEven_estado %></td>
                        <td><%= listaCtrlEven[i].prodSaida_id %></td>
                        <td><%= listaCtrlEven[i].prodSaida_qnt %></td>
                        <td><%= listaCtrlEven[i].prodEntrada_id %></td>
                        <td><%= listaCtrlEven[i].prodEntrada_qnt %></td>
                        <td><%= listaCtrlEven[i].even_id %></td>
                        <td><%= listaCtrlEven[i].createdAt %></td>
                        <td><%= listaCtrlEven[i].updatedAt %></td>
                        <td>
                            <a href="/ctrlSaidaEvento/alterar/<%= listaCtrlEven[i].ctrlEven_id %>" class="btn btn-primary"><i class="fas fa-pen"></i></a>
                            <button data-codigoexclusao="<%= listaCtrlEven[i].ctrlEven_id %>" class="btn btn-danger btnExclusao"><i class="fas fa-trash"></i></button>
                        </td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>